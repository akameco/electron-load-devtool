{
  "name": "electron-load-devtool",
  "version": "1.2.0",
  "description": "Easily load dev-tool for electron",
  "license": "MIT",
  "repository": "akameco/electron-load-devtool",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "akameco.github.io"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "start": "electron fixture.js",
    "test": "eslint . --fix"
  },
  "lint-staged": {
    "*.js": [
      "fmt",
      "eslint --fix .",
      "git add"
    ],
    "*.json": [
      "fmt",
      "git add"
    ],
    "*.md": [
      "fmt",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "electron",
    "chrome",
    "extention"
  ],
  "dependencies": {
    "electron-is-dev": "^1.0.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.6.0",
    "electron": "^4.0.1",
    "eslint": "^5.12.0",
    "eslint-config-precure": "^3.4.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
